DEFINITION MODULE Clock;

   PROCEDURE ResetClock;

   PROCEDURE UserTime() : INTEGER;
      (* Return user time since last 'ResetClock' *)

   PROCEDURE SystemTime() : INTEGER;
      (* Return system time since last 'ResetClock' *)


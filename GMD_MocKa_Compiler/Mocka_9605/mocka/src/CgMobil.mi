     (* RULE 242/2410 *)
    c:=  2+     e^.son[1]^.gcg^.cost[ntarglist]+
     e^.son[2]^.gcg^.cost[ntRegOrCMemOrIm];
   IF c <   ga^.cost[ntarglist]  THEN 
   IF  SizeTable[e^.attr^.Pass.mode] = 4  THEN 
     ga^.cost[ntarglist] := c; 
     ga^.rule[ntarglist] := 242;
   END;
   END;
     (* RULE 243/2428 *)
    c:=  2+     e^.son[1]^.gcg^.cost[ntarglist]+

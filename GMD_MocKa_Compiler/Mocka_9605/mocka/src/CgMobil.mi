   END;
     (* RULE 244/2434 *)
    c:=  4+     e^.son[1]^.gcg^.cost[ntarglist]+
     e^.son[2]^.gcg^.cost[ntCMem];
   IF c <   ga^.cost[ntarglist]  THEN 
   IF  e^.attr^.Pass.mode = FloatLong  THEN 
     ga^.cost[ntarglist] := c; 
     ga^.rule[ntarglist] := 244;
   END;
   END;
     (* RULE 245/2444 *)
    c:=  47+     e^.son[1]^.gcg^.cost[ntarglist]+
     e^.son[2]^.gcg^.cost[ntfreg];

    c:=  2+     e^.son[1]^.gcg^.cost[ntreg]+
     e^.son[2]^.gcg^.cost[ntRegOrCMemOrIm];
   IF c <   ga^.cost[ntreg]  THEN 
 ga^.reg.mode := e^.attr^.FixedMinus.mode; ;
     ga^.cost[ntreg] := c; 
     ga^.rule[ntreg] := 95;

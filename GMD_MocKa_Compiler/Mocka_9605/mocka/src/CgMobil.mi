 PROCEDURE  Content (
                      Atmode  :  CgBase.Mode
                      ;  op1  : Address
                      ;  VAR result : Data);
 VAR    e  : IR.Expression;
        a  : IR.Attributes;
        ga : IR.ExprAttributes;
        c  : INTEGER;
        e2,e3,e4,e5,e6,e7,e8,e9,e10 : IR.Expression;

 BEGIN
   IF NextAttr=NIL THEN MemExtend; END;
   e := NextExpr;
   a := NextAttr;
   ga  := NextGcg; e^.attr := a;
   NextAttr := NextAttr^.hashchain;
   NextExpr := NextExpr^.hashchain;
   NextGcg  := NextGcg^.hashchain; 
  a^.op := IR.opContent;
   a^.Content.mode := Atmode;
   e^.arity := 1;
   e^.son[1] := op1;
   result := e;
   ga^.cost := IR.InfCosts;
   e^.gcg := ga;

     (* RULE 41/681 *)
    c:=       e^.son[1]^.gcg^.cost[ntmem];
   IF c <   ga^.cost[ntCMem]  THEN 
 ga^.CMem.mode := e^.attr^.Content.mode; ;
     ga^.cost[ntCMem] := c; 
     ga^.rule[ntCMem] := 41;
   END;
   MatchChainRules(e);
 END Content;


 PROCEDURE  Assign (
                      Atmode  :  CgBase.Mode
                      ;  op1  : Address
                      ;  op2  : Data
                                                         );
 VAR    e  : IR.Expression;
        a  : IR.Attributes;
        ga : IR.ExprAttributes;
        c  : INTEGER;
        e2,e3,e4,e5,e6,e7,e8,e9,e10 : IR.Expression;

 BEGIN
   IF NextAttr=NIL THEN MemExtend; END;
   e := NextExpr;
   a := NextAttr;
   ga  := NextGcg; e^.attr := a;
   NextAttr := NextAttr^.hashchain;
   NextExpr := NextExpr^.hashchain;
   NextGcg  := NextGcg^.hashchain; 
  a^.op := IR.opAssign;
   a^.Assign.mode := Atmode;
   e^.arity := 2;
   e^.son[1] := op1;
   e^.son[2] := op2;
   ga^.stmtcost := IR.infcost;
   e^.gcg := ga;

     (* RULE 42/688 *)
    c:=  2+     e^.son[1]^.gcg^.cost[ntmem]+
     e^.son[2]^.gcg^.cost[ntRegOrIm];
   IF c <   ga^.stmtcost  THEN 
   IF  e^.attr^.Assign.mode < FloatShort  THEN 
     ga^.stmtcost := c; 
     ga^.stmtrule := 42;
   END;
   END;
     (* RULE 43/693 *)
    c:=  45+     e^.son[1]^.gcg^.cost[ntmem]+
     e^.son[2]^.gcg^.cost[ntfreg];
   IF c <   ga^.stmtcost  THEN 
   IF  e^.attr^.Assign.mode >= FloatShort  THEN 
     ga^.stmtcost := c; 
     ga^.stmtrule := 43;
   END;
   END;
     (* RULE 44/701 *)
    c:=  2+     e^.son[1]^.gcg^.cost[ntmem]+
     e^.son[2]^.gcg^.cost[ntFloatConstant];
   IF c <   ga^.stmtcost  THEN 
   IF  e^.attr^.Assign.mode = FloatShort  THEN 
     ga^.stmtcost := c; 
     ga^.stmtrule := 44;
   END;
   END;
     (* RULE 45/709 *)
    c:=  4+     e^.son[1]^.gcg^.cost[ntmem]+
     e^.son[2]^.gcg^.cost[ntFloatConstant];
   IF c <   ga^.stmtcost  THEN 
   IF  e^.attr^.Assign.mode = FloatLong  THEN 
     ga^.stmtcost := c; 
     ga^.stmtrule := 45;
   END;
   END;
     (* RULE 46/719 *)
    c:=  4+     e^.son[1]^.gcg^.cost[ntmem]+
     e^.son[2]^.gcg^.cost[ntCMem];
   IF c <   ga^.stmtcost  THEN 

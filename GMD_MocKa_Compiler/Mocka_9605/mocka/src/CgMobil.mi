 PROCEDURE  CheckL (
                      AtIndexMode  :  CgBase.Mode;
                      AtLwbMode  :  CgBase.Mode;
                      AtUpbMode  :  CgBase.Mode;
                      AtCheckLwb  :  BOOLEAN;
                      AtCheckUpb  :  BOOLEAN
                      ;  op1  : Data
                      ;  op2  : Data
                      ;  op3  : Data
                      ;  VAR result : Data);
 VAR    e  : IR.Expression;
        a  : IR.Attributes;
        ga : IR.ExprAttributes;
        c  : INTEGER;
        e2,e3,e4,e5,e6,e7,e8,e9,e10 : IR.Expression;

 BEGIN
   IF NextAttr=NIL THEN MemExtend; END;
   e := NextExpr;
   a := NextAttr;
   ga  := NextGcg; e^.attr := a;

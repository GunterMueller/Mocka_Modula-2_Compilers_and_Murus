     (* RULE 98/1115 *)
   e2 := e^.son[1];
   IF e2^.attr^.op = opContent THEN 
    c:=       e2^.son[1]^.gcg^.cost[ntSimpleVariable];
   e2 := e^.son[2];
   IF e2^.attr^.op = opContent THEN 
    c:=  c+     e2^.son[1]^.gcg^.cost[ntSimpleVariable];
   IF c <   ga^.cost[ntConstant]  THEN 

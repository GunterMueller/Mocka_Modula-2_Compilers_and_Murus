     ga^.cost[ntreg] := c; 
     ga^.rule[ntreg] := 145;
   END;
   END;
     (* RULE 146/1435 *)
    c:=  14+     e^.son[1]^.gcg^.cost[ntreg]+
     e^.son[2]^.gcg^.cost[ntreg];
   IF c <   ga^.cost[ntreg]  THEN 
   IF  e^.attr^.FixedDiv.mode = SignedWord  THEN 
 ga^.reg.mode := e^.attr^.FixedDiv.mode; ;
     ga^.cost[ntreg] := c; 
     ga^.rule[ntreg] := 146;
   END;
   END;
   MatchChainRules(e);

     (* RULE 62/914 *)
    c:=  11+     e^.son[1]^.gcg^.cost[ntreg];
   IF c <   ga^.cost[ntreg]  THEN 
   IF  SignedTable[e^.attr^.FixedAbs.mode]  THEN 
 ga^.reg.mode := e^.attr^.FixedAbs.mode; ;
     ga^.cost[ntreg] := c; 
     ga^.rule[ntreg] := 62;
   END;

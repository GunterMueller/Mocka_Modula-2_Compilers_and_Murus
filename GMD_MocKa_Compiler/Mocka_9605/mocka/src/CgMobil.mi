	  EqualMemAdr (e^.son[1]^.son[1]^.gcg^.SimpleVariable.index, e^.son[2]^.son[1]^.gcg^.SimpleVariable.index)  THEN 
 ga^.Constant.val := 0;
	  ga^.Constant.mode := e^.attr^.FixedMinus.mode; ;
     ga^.cost[ntConstant] := c; 
     ga^.rule[ntConstant] := 98;
   END;
   END;
   END;
   END;
   MatchChainRules(e);
 END FixedMinus;


